                     , text                     >= 0.11       && < 1.0
                     , dtd-types                >= 0.3        && < 0.4
                     , dtd-text                 >= 0.1.1.1    && < 0.2
                     , containers               >= 0.2        && < 0.5
                     , xml-enumerator           >= 0.4        && < 0.5
                     , bytestring               >= 0.9        && < 0.10
                     , enumerator               >= 0.4.14     && < 0.5
                     , uri-enumerator           >= 0.0        && < 0.1
                     , transformers             >= 0.2        && < 0.3
  Ghc-options: -Wall
